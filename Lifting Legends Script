EggSector:Cheat(
	"Checkbox",
	"Auto Buy Teir 1 Egg (10 K)",
	function(State)
		Teir1 = State;
	end)
spawn(function()
	while wait() do
		if Teir1 then
			local A_1 = "PurchaseCrate"
			local A_2 = "Tier1"
			local A_3 = 1
			local Event = game:GetService("ReplicatedStorage").Network.Port1
			Event:FireServer(A_1, A_2, A_3)
			wait(1.5)
		end
	end	
end)

