local Finity = loadstring(game:HttpGet("https://pastebin.com/raw/vHJa2WhD"))()
local FinityWindow = Finity.new(true, "Lift Legends | Made By BigHazzar#3081 | Toggle with RShift", UDim2.new(0, 600, 0, 400))
FinityWindow.ChangeToggleKey(Enum.KeyCode.RightShift)

local NameCategory = FinityWindow:Category("Credits")
local AutoFarmCategory = FinityWindow:Category("AutoFarms")
local ShopCategory = FinityWindow:Category("Shop")
local TeleportsCategory = FinityWindow:Category("Teleports")
local MISCCategory = FinityWindow:Category("MISC")


local AutoFarmSector = AutoFarmCategory:Sector("Auto Farms")
local TeleportSector = TeleportsCategory:Sector("Zone Teleports")
local AutoBuySector = ShopCategory:Sector("Auto Buy Weight Stuff")
local CreditsSector = NameCategory:Sector("Script Maker")
local EggSector = ShopCategory:Sector("Buy Pet Stuff (No Animation)")
local MISCSector = MISCCategory:Sector("MISC Stuff")

AutoFarmSector:Cheat(
	"Checkbox",
	"Auto Lift",
	function(State)
		name = State;
	end)
spawn(function()
	while wait() do
		if name then
			local A_1 = "Click"
			local Event = game:GetService("ReplicatedStorage").Network.Port1
			Event:FireServer(A_1)
			wait(0.5)
		end
	end
end)

AutoFarmSector:Cheat(
	"Checkbox",
	"Auto Train Speed",
	function(State)
		AutoSpeed = State;
	end)
spawn(function()
	while wait() do
		if AutoSpeed then
			local A_1 = "Running"
			local Event = game:GetService("ReplicatedStorage").Network.Port1
			Event:FireServer(A_1)
			wait()
		end
	end
end)

AutoFarmSector:Cheat(
	"Checkbox",
	"Auto Train Punch",
	function(State)
		AutoPunch = State;
	end)
spawn(function()
	while wait() do
		if AutoPunch then
			local A_1 = "Punching"
			local A_2 = game:GetService("Workspace").PowerTrainingFolder.Model
			local Event = game:GetService("ReplicatedStorage").Network.Port1
			Event:FireServer(A_1, A_2)
			wait()
		end
	end
end)

