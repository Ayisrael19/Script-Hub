BoxHandleAdornment.Transparency = 0
BoxHandleAdornment.ZIndex = 2
BoxHandleAdornment.Parent = LeftLowerArmEsp

game.Players.PlayerAdded:Connect(function(plr)
	if plr.Name ~= game.Players.LocalPlayer.Name then
	TorsoEsp:Clone()
	TorsoEsp.Parent = game.Workspace:WaitForChild(plr.Name)
	TorsoEsp.BoxHandleAdornment.Adornee = game.Workspace:WaitForChild(plr.Name).HumanoidRootPart
	wait()
	HeadEsp:Clone()
	HeadEsp.Parent = game.Workspace:WaitForChild(plr.Name)
	HeadEsp.BoxHandleAdornment.Adornee = game.Workspace:WaitForChild(plr.Name).Head
	wait()
	UpperTorsoEsp:Clone()
	UpperTorsoEsp.Parent = game.Workspace:WaitForChild(plr.Name)
	UpperTorsoEsp.BoxHandleAdornment.Adornee = game.Workspace:WaitForChild(plr.Name).UpperTorso
	wait()
	RightUpperLegEsp:Clone()
	RightUpperLegEsp.Parent = game.Workspace:WaitForChild(plr.Name)
	RightUpperLegEsp.BoxHandleAdornment.Adornee = game.Workspace:WaitForChild(plr.Name).RightUpperLeg
	wait()
	LeftUpperLegEsp:Clone()
	LeftUpperLegEsp.Parent = game.Workspace:WaitForChild(plr.Name)
	LeftUpperLegEsp.BoxHandleAdornment.Adornee = game.Workspace:WaitForChild(plr.Name).LeftUpperLeg
	wait()
	LowerTorsoEsp:Clone()
	LowerTorsoEsp.Parent = game.Workspace:WaitForChild(plr.Name)
	LowerTorsoEsp.BoxHandleAdornment.Adornee = game.Workspace:WaitForChild(plr.Name).LowerTorso
	wait()
	RightLowerArmEsp:Clone()
	RightLowerArmEsp.Parent = game.Workspace:WaitForChild(plr.Name)
	RightLowerArmEsp.BoxHandleAdornment.Adornee = game.Workspace:WaitForChild(plr.Name).RightLowerArm
	wait()
	LeftLowerArmEsp:Clone()
	LeftLowerArmEsp.Parent = game.Workspace:WaitForChild(plr.Name)
	LeftLowerArmEsp.BoxHandleAdornment.Adornee = game.Workspace:WaitForChild(plr.Name).LeftLowerArm
	wait()
	end
end)


for i, v in pairs(gplrs) do
	if v.Name ~= plr.Name then
	local BoxHandleAdornment = Instance.new("BoxHandleAdornment")
	BoxHandleAdornment.Adornee = v.Character.HumanoidRootPart
	BoxHandleAdornment.AlwaysOnTop = true
	BoxHandleAdornment.Color3 = Color3.fromRGB(170, 0, 0)
	BoxHandleAdornment.Size = Vector3.new(0.2, 1.7, 0.2)
	BoxHandleAdornment.Transparency = 0
	BoxHandleAdornment.ZIndex = 2
	BoxHandleAdornment.Parent = v.Character
	end
end


for i, v in pairs(gplrs) do
	if v.Name ~= plr.Name then
	local BoxHandleAdornment = Instance.new("BoxHandleAdornment")
	BoxHandleAdornment.Adornee = v.Character.Head
	BoxHandleAdornment.AlwaysOnTop = true
	BoxHandleAdornment.Color3 = Color3.fromRGB(170, 0, 0)
	BoxHandleAdornment.Size = Vector3.new(0.5, 1.2, 0.2)
	BoxHandleAdornment.Transparency = 0
	BoxHandleAdornment.ZIndex = 2
	BoxHandleAdornment.Parent = v.Character
	end
end


for i, v in pairs(gplrs) do
	if v.Name ~= plr.Name then
	local BoxHandleAdornment = Instance.new("BoxHandleAdornment")
	BoxHandleAdornment.Adornee = v.Character.UpperTorso
	BoxHandleAdornment.AlwaysOnTop = true
	BoxHandleAdornment.Color3 = Color3.fromRGB(170, 0, 0)
	BoxHandleAdornment.Size = Vector3.new(3, 0.2, 0.2)
	BoxHandleAdornment.Transparency = 0
	BoxHandleAdornment.ZIndex = 2
	BoxHandleAdornment.Parent = v.Character
	end
end


for i, v in pairs(gplrs) do
	if v.Name ~= plr.Name then
	local BoxHandleAdornment = Instance.new("BoxHandleAdornment")
	BoxHandleAdornment.Adornee = v.Character.RightUpperLeg
	BoxHandleAdornment.AlwaysOnTop = true
	BoxHandleAdornment.Color3 = Color3.fromRGB(170, 0, 0)
	BoxHandleAdornment.Size = Vector3.new(0.2, 1.2, 0.2)
	BoxHandleAdornment.Transparency = 0
	BoxHandleAdornment.ZIndex = 2
	BoxHandleAdornment.Parent = v.Character
	end
end


for i, v in pairs(gplrs) do
	if v.Name ~= plr.Name then
	local BoxHandleAdornment = Instance.new("BoxHandleAdornment")
	BoxHandleAdornment.Adornee = v.Character.LeftUpperLeg
	BoxHandleAdornment.AlwaysOnTop = true
	BoxHandleAdornment.Color3 = Color3.fromRGB(170, 0, 0)
	BoxHandleAdornment.Size = Vector3.new(0.2, 1.2, 0.2)
	BoxHandleAdornment.Transparency = 0
	BoxHandleAdornment.ZIndex = 2
	BoxHandleAdornment.Parent = v.Character
	end
end


for i, v in pairs(gplrs) do
	if v.Name ~= plr.Name then
	local BoxHandleAdornment = Instance.new("BoxHandleAdornment")
	BoxHandleAdornment.Adornee = v.Character.LowerTorso
	BoxHandleAdornment.AlwaysOnTop = true
	BoxHandleAdornment.Color3 = Color3.fromRGB(170, 0, 0)
	BoxHandleAdornment.Size = Vector3.new(1.1, 0.2, 0.2)
	BoxHandleAdornment.Transparency = 0
	BoxHandleAdornment.ZIndex = 2
	BoxHandleAdornment.Parent = v.Character
		end
	end
	

for i, v in pairs(gplrs) do
	if v.Name ~= plr.Name then
	local BoxHandleAdornment = Instance.new("BoxHandleAdornment")
	BoxHandleAdornment.Adornee = v.Character.RightLowerArm
	BoxHandleAdornment.AlwaysOnTop = true
	BoxHandleAdornment.Color3 = Color3.fromRGB(170, 0, 0)
	BoxHandleAdornment.Size = Vector3.new(0.2, 1, 0.2)
	BoxHandleAdornment.Transparency = 0
	BoxHandleAdornment.ZIndex = 2
	BoxHandleAdornment.Parent = v.Character
	end
end


		


for i, v in pairs(gplrs) do
	if v.Name ~= plr.Name then
	local BoxHandleAdornment = Instance.new("BoxHandleAdornment")
	BoxHandleAdornment.Adornee = v.Character.LeftLowerArm
	BoxHandleAdornment.AlwaysOnTop = true
	BoxHandleAdornment.Color3 = Color3.fromRGB(170, 0, 0)
	BoxHandleAdornment.Size = Vector3.new(0.2, 1, 0.2)
	BoxHandleAdornment.Transparency = 0
	BoxHandleAdornment.ZIndex = 2
	BoxHandleAdornment.Parent = v.Character
	end
end





			
			

wait()
local plr = game.Players.LocalPlayer
game.Workspace:WaitForChild(plr.Name).BoxHandleAdornment:Destroy()

