MISCSector:Cheat("Slider", "Speed", function(Value)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	print("Silder value changed:", Value)
end, {min = 0, max = 200, suffix = "Speed"})

MISCSector:Cheat("Slider", "Jump Power", function(Value)
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	print("Silder value changed:", Value)
end, {min = 0, max = 200, suffix = "Jump Power"})

